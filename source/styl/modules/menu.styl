@media (max-width 769px)

  nav
    position fixed
    top 0
    right -100%
    background white
    width 100%
    display block
    z-index 11
    height 100%
    font-size 22px
    transition: .4s
    overflow auto
    &.visible
      right 0;
      -webkit-box-shadow: -3px 5px 11px 0px rgba(0,0,0,0.38);
      -moz-box-shadow: -3px 5px 11px 0px rgba(0,0,0,0.38);
      box-shadow: -3px 5px 11px 0px rgba(0,0,0,0.38);
    ul
      li
        display block
        padding 20px
        margin 0
        font-size 28px
        a
          text-decoration none
          color #15236b
          &:hover
            text-decoration underline
            color #15236b
          font-family: 'BentonSans-medium';
      li:first-child
        margin-top 70px
  .menuuu
    display block
    button
      background: transparent;
      width 40px
      height 40px
      margin-right 10px
      border-radius 50%
      position fixed 
      border 2px solid #15236b
      -webkit-box-shadow: 2px 10px 14px -6px rgba(0,0,0,0.75);
      -moz-box-shadow: 2px 10px 14px -6px rgba(0,0,0,0.75);
      box-shadow: 2px 10px 14px -6px rgba(0,0,0,0.75);
  .btn-menu{
    border: none;
    outline: none;
    display: block !important;
    position: absolute;
    top: 26px;
    right: 10px;
    z-index: 20;
    width: 40px;
    height: 25px;
    -webkit-transform: rotate(0deg);
    -moz-transform: rotate(0deg);
    -o-transform: rotate(0deg);
    transform: rotate(0deg);
    -webkit-transition: .5s ease-in-out;
    -moz-transition: .5s ease-in-out;
    -o-transition: .5s ease-in-out;
    transition: .5s ease-in-out;
    cursor: pointer;
    background: transparent
      
    }

  .btn-menu span {
    display: block;
    position: absolute;
    height: 3px;
    width: 50%;
    background: #15236b;
    opacity: 1;
    left: 9px;
    -webkit-transform: rotate(0deg);
    -moz-transform: rotate(0deg);
    -o-transform: rotate(0deg);
    transform: rotate(0deg);
    -webkit-transition: .25s ease-in-out;
    -moz-transition: .25s ease-in-out;
    -o-transition: .25s ease-in-out;
    transition: .25s ease-in-out;
    
    
  }

  .bck-white{
    background: white !important;
  }

  .btn-menu span:nth-child(1) {
    top: 10px;
    -webkit-box-shadow: 2px 10px 14px -6px rgba(0,0,0,0.75);
    -moz-box-shadow: 2px 10px 14px -6px rgba(0,0,0,0.75);
    box-shadow: 2px 10px 14px -6px rgba(0,0,0,0.75);
  }

  .btn-menu span:nth-child(2),.btn-menu span:nth-child(3) {
    top: 17px;
    -webkit-box-shadow: 2px 10px 14px -6px rgba(0,0,0,0.75);
    -moz-box-shadow: 2px 10px 14px -6px rgba(0,0,0,0.75);
    box-shadow: 2px 10px 14px -6px rgba(0,0,0,0.75);
  }

  .btn-menu span:nth-child(4) {
    top: 24px;
    -webkit-box-shadow: 2px 10px 14px -6px rgba(0,0,0,0.75);
    -moz-box-shadow: 2px 10px 14px -6px rgba(0,0,0,0.75);
    box-shadow: 2px 10px 14px -6px rgba(0,0,0,0.75);
  }

  .btn-menu.open span:nth-child(1) {
    top: 18px;
    width: 0%;
    left: 50%;
    color black
    -webkit-box-shadow: 2px 10px 14px -6px rgba(0,0,0,0.75);
    -moz-box-shadow: 2px 10px 14px -6px rgba(0,0,0,0.75);
    box-shadow: 2px 10px 14px -6px rgba(0,0,0,0.75);
  }

  .btn-menu.open span:nth-child(2) {
    -webkit-transform: rotate(45deg);
    -moz-transform: rotate(45deg);
    -o-transform: rotate(45deg);
    transform: rotate(45deg);
    color black
  }

  .btn-menu.open span:nth-child(3) {
    -webkit-transform: rotate(-45deg);
    -moz-transform: rotate(-45deg);
    -o-transform: rotate(-45deg);
    transform: rotate(-45deg);
    color black
  }

  .btn-menu.open span:nth-child(4) {
    top: 18px;
    width: 0%;
    left: 50%;
    color black
  }
